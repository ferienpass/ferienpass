{
  "name": "ferienpass/admin-bundle",
  "license": "proprietary",
  "description": "",
  "type": "contao-bundle",
  "homepage": "https://ferienpass.online/",
  "authors": [
    {
      "name": "Richard Henkenjohann",
      "email": "info@ferienpass.online",
      "homepage": "https://ferienpass.online",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "info@ferienpass.online"
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*",

    "contao/core-bundle": "^5.1",
    "ferienpass/core-bundle": "dev-main as 4.0.0",
    "ferienpass/ux-host-portal": "dev-main as 4.0.0",

    "knplabs/knp-menu-bundle": "^3.0",
    "michelf/php-markdown": "^1.9",
    "psr/log": "^1.1",
    "symfony/asset": "^5.2 || ^6.1",
    "symfony/config": "^5.2 || ^6.1",
    "symfony/dependency-injection": "^5.2 || ^6.1",
    "symfony/framework-bundle": "^5.2 || ^6.1",
    "symfony/form": "^5.2 || ^6.1",
    "symfony/http-foundation": "^5.2 || ^6.1",
    "symfony/http-kernel": "^5.2 || ^6.1",
    "symfony/messenger": "^5.2 || ^6.1",
    "symfony/password-hasher": "^5.3 || ^6.1",
    "symfony/security-bundle": "^5.3 || ^6.1",
    "symfony/serializer": "^5.3 || ^6.1",
    "symfony/routing": "^5.2 || ^6.1",
    "symfony/security-core": "^5.3 || ^6.1",
    "symfony/security-csrf": "^5.2 || ^6.1",
    "symfony/security-http": "^5.2 || ^6.1",
    "symfony/translation": "^5.2 || ^6.1",
    "symfony/validator": "^5.2 || ^6.1",
    "phpoffice/phpspreadsheet": "^1.29",
    "twig/twig": "^2.12 || ^3.0",
    "twig/intl-extra": "^2.14 || ^3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.ferienpass.online/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ferienpass\\AdminBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Ferienpass\\AdminBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "php-http/discovery": true,
      "contao/manager-plugin": true,
      "contao-community-alliance/composer-plugin": true
    }
  }
}
