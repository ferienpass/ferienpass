{
  "name": "ferienpass/cms-bundle",
  "license": "proprietary",
  "description": "",
  "keywords": [
    "symfony-ux"
  ],
  "type": "symfony-bundle",
  "homepage": "https://ferienpass.online/",
  "authors": [
    {
      "name": "Richard Henkenjohann",
      "email": "info@ferienpass.online",
      "homepage": "https://ferienpass.online",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "info@ferienpass.online"
  },
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-json": "*",
    "ext-pdo": "*",

    "contao/core-bundle": "^5.3@rc",
    "contao/faq-bundle": "^5.3@rc",
    "richardhj/contao-knp-menu": "^1.0",
    "richardhj/contao-theme-framework": "3.x-dev@dev",
    "symfony/ux-live-component": "^2.0",
    "symfony/stimulus-bundle": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "Ferienpass\\CmsBundle\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-4.x": "4.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": false,
      "php-http/discovery": true,
      "contao/manager-plugin": false
    }
  }
