{
  "name": "ferienpass/ferienpass",
  "type": "symfony-bundle",
  "description": "",
  "homepage": "https://ferienpass.online",
  "license": "proprietary",
  "authors": [
    {
      "name": "Richard Henkenjohann",
      "email": "info@ferienpass.online",
      "homepage": "https://ferienpass.online",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "info@ferienpass.online"
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-pdo": "*",

    "contao/core-bundle": "^5.0",
    "contao/news-bundle": "^5.0",
    "contao/manager-bundle": "^5.0",


    "mvo/contao-group-widget": "^1.3",
    "richardhj/contao-knp-menu": "^1.0",
    "richardhj/contao-theme-framework": "^2.0",


    "eluceo/ical": "^2.1",
    "league/flysystem-sftp-v3": "^3.0",
    "mpdf/mpdf": "^8.0",
    "knplabs/knp-snappy-bundle": "^1.7",
    "phpoffice/phpspreadsheet": "^1.29",
    "phpoffice/phpword": "^0.18",
    "michelf/php-markdown": "^1.8",
    "psr/log": "^1.0",
    "friendsofsymfony/http-cache-bundle": "^2.7",
    "terminal42/service-annotation-bundle": "^1.0",
    "odolbeau/phone-number-bundle": "^3.0",

    "beberlei/doctrineextensions": "^1.3",
    "monolog/monolog": "^2.0",
    "sensio/framework-extra-bundle": "^6.1",
    "symfony/asset": "^5.2 || ^6.0",
    "symfony/config": "^5.2 || ^6.0",
    "symfony/dependency-injection": "^5.2 || ^6.0",
    "symfony/doctrine-bridge": "^5.2 || ^6.0",
    "symfony/event-dispatcher": "^5.2 || ^6.0",
    "symfony/filesystem": "^5.2 || ^6.0",
    "symfony/form": "^5.2 || ^6.0",
    "symfony/options-resolver": "^5.2 || ^6.0",
    "symfony/framework-bundle": "^5.2 || ^6.0",
    "symfony/http-foundation": "^5.3 || ^6.0",
    "symfony/http-kernel": "^5.2 || ^6.0",
    "symfony/messenger": "^5.2 || ^6.0",
    "symfony/notifier": "^5.2 || ^6.0",
    "symfony/password-hasher": "^5.3 || ^6.0",
    "symfony/security-bundle": "^5.3 || ^6.0",
    "symfony/routing": "^5.2 || ^6.0",
    "symfony/security-core": "^5.3 || ^6.0",
    "symfony/security-csrf": "^5.2 || ^6.0",
    "symfony/security-http": "^5.2 || ^6.0",
    "symfony/serializer": "^5.2 || ^6.0",
    "symfony/templating": "^5.2 || ^6.0",
    "symfony/translation": "^5.2 || ^6.0",
    "symfony/twig-bridge": "^5.2 || ^6.0",
    "symfony/ux-live-component": "^2.0",
    "symfony/validator": "^5.2 || ^6.0",
    "twig/twig": "^2.12 || ^3.0",
    "twig/intl-extra": "^2.14 || ^3.0",
    "twig/cssinliner-extra": "^3.6",
    "twig/inky-extra": "^3.6",
    "knplabs/knp-menu-bundle": "^3.0",
    "zenstruck/foundry": "^1.10",
    "symfony/stimulus-bundle": "^2.9"
  },
  "suggest": {
    "spatie/pdf-to-image": "To generate images from PDF Print Sheets"
  },
  "replace": {
    "ferienpass/base-bundle": "self.version",
    "ferienpass/backup-sftp-bundle": "self.version",
    "ferienpass/core-bundle": "self.version",
    "ferienpass/fixtures-bundle": "self.version",
    "ferienpass/admin-bundle": "self.version",
    "ferienpass/ux-theme": "self.version",
    "ferienpass/ux-host-portal": "self.version"
  },
  "extra": {
    "contao-manager-plugin": {
      "ferienpass/fixtures-bundle": "Ferienpass\\FixturesBundle\\ContaoManager\\Plugin",
      "ferienpass/backup-sftp-bundle": "Ferienpass\\BackupSftpBundle\\ContaoManager\\Plugin",
      "ferienpass/core-bundle": "Ferienpass\\CoreBundle\\ContaoManager\\Plugin",
      "ferienpass/admin-bundle": "Ferienpass\\AdminBundle\\ContaoManager\\Plugin",
      "ferienpass/ux-host-portal": "Ferienpass\\UX\\HostPortal\\ContaoManager\\Plugin"
    },
    "symfony": {
      "require": "^5.2"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Ferienpass\\BackupSftpBundle\\": "backup-sftp-bundle/src/",
      "Ferienpass\\CoreBundle\\": "core-bundle/src/",
      "Ferienpass\\FixturesBundle\\": "fixtures-bundle/src/",
      "Ferienpass\\AdminBundle\\": "admin-bundle/src/",
      "Ferienpass\\UX\\HostPortal\\": "ux/packages/HostPortal/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ferienpass\\CoreBundle\\Tests\\": "core-bundle/tests/",
      "Ferienpass\\CoreBundle\\Fixtures\\": "core-bundle/tests/Fixtures/src/",
      "Ferienpass\\AdminBundle\\Tests\\": "admin-bundle/tests/"
    }
  },
  "require-dev": {
    "vimeo/psalm": "^5.12",
    "psalm/plugin-symfony": "^5.0",
    "weirdan/doctrine-psalm-plugin": "^2.8",
    "friendsofphp/php-cs-fixer": "^3.17",
    "phpunit/phpunit": "^10.2",
    "symfony/test-pack": "^1.0",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "dama/doctrine-test-bundle": "^6.5",
    "rector/rector": "^0.17.1"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "contao-community-alliance/composer-plugin": true,
      "contao/manager-plugin": true,
      "php-http/discovery": true
    }
  }
}
